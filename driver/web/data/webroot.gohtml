<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="UTF-8">
    <link type="text/css" rel="stylesheet" href="/exciton/web/assets/webroot.css"></link>
</head>

<body>
    <nav id="menubar" class="navbar"></nav>
    <div id="pagecontainer"></div>
    <script>window.exciton = { ID: {{.ID}} };</script>
    <script src="/exciton/web/assets/exciton-ws.js"></script>

    <dialog id="fileOpenDialog">
        <h3 id="fileOpenDialogTitle"></h3>
        <div class="dialogContent">
            <form id="fileUploadForm" enctype="multipart/form-data">
            <input type="file" id="selFile" name="selFile"></input>
            <input type="hidden" id="openDialogResponceNo" name="openDialogResponceNo" value="0"></input>
            </form>
        </div>
        <footer>
        <form method="dialog">
            <button value="ok" id="fileOpenOK">Upload</button>
            <button value="cancel">Cancel</button>
        </form>
        </footer>
    </dialog>
    <dialog id="messageBox">
    <h3 id="messageBoxTitle"></h3>
    <div class="dialogContent">
        <p><img width="32" height="32" id="messageBoxIcon"><span id="messageBoxContent"></span></p>
        <p id="messageBoxDetail" class="dialogContentDetail"></p>
    </div>
    <footer>
        <form method="dialog" id="messageBoxButtons">
            <!--input class="dialogCloseButton" type="submit" value="Close"-->
        </form>
    </dialog>
</body>

</html>